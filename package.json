    "vue-router": "^3.5.3",
    "vuex": "^3.6.2",
    "wangeditor": "^4.7.10",
    "webpack": "^4.46.0",
    "webpackbar": "^5.0.2",
    "zx-count": "^0.3.7",
    "zx-layouts": "^0.6.27",
